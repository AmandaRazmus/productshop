{
  "name": "productshop",
  "version": "1.0.0",
  "description": "MERN shopping cart app",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "concurrently \"react-scripts start\" \"cd backend && nodemon server\"",
    "build": "react-scripts build",
    "server": "nodemon backend/server.js",
    "client": "npm start --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run client\" ",
    "data:import": "node backend/seeder.js",
    "data: destroy": "node backend/seeder.js -d"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/AmandaRazmus/ProductShop.git"
  },
  "author": "Amanda Razmus",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/AmandaRazmus/ProductShop/issues"
  },
  "homepage": "https://github.com/AmandaRazmus/relativepath",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "concurrently": "^8.0.1",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^7.1.1",
    "nodemon": "^3.0.1",
    "path": "^0.12.7",
    "react-scripts": "^5.0.1"
  }
}
